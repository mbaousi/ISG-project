    echo "policy-map type service $name1"
    echo " 9 class type traffic INTERNAL"
    echo "  prepaid config IBS"
    echo "  police input $speed1 $speed1 $speed1"
    echo "  police output $speed1 $speed1 $speed1"
    echo ' !'
    echo " class type traffic default in-out"
    echo "drop"
    echo "!"
    echo "!"
done < $file1



while IFS=, read name speed
do
    echo "policy-map type service $name"
    echo " 10 class type traffic INTERNET"
    echo "  prepaid config IBS"
    echo "  police input $speed $speed $speed"
    echo "  police output $speed $speed $speed"
    echo ' !'
    echo " class type traffic default in-out"
    echo "drop"
    echo "!"
    echo "!"
done < $file



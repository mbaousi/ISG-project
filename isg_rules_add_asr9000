  echo 'accounting aaa list ibs-isg type service periodic-interval $interm'
  echo 'ipv4 access-group INTERNET ingress'
  echo 'ipv4 access-group INTERNET egress'
 echo '!'
 echo 'type service INTRANET'
  echo "accounting aaa list ibs-isg type service periodic-interval $interm"
  echo 'ipv4 access-group INTRANET ingress'
  echo 'ipv4 access-group INTRANET egress'
 echo '!'



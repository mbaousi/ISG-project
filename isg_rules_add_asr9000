while IFS=, read name
do
echo "policy-map INTERNET__$name"
 echo 'class INTERNET'
  echo "police rate $name kbps"
  echo '!'
 echo '!'
 echo 'class class-default'
 echo '!'
 echo 'end-policy-map'
echo '!'
echo "policy-map INTERNAL__$name"
 echo 'class INTERNAL'
  echo "police rate $name kbps"
  echo '!'
 echo '!'
 echo 'class class-default'
 echo '!'
 echo 'end-policy-map'
echo '!'


done < $file

while IFS=, read name
do

echo 'dynamic-template'
echo "type service INTERNET__$name"
  echo "service-policy input INTERNET__$name merge 30 acct-stats"
  echo "service-policy output INTERNET__$name merge 30 acct-stats"
  echo "accounting aaa list ibs-isg type service periodic-interval $interm"
 echo 'ipv4 access-group INTERNET ingress'
  echo 'ipv4 access-group INTERNET egress'

echo "type service INTERNAL__$name"
  echo "service-policy input INTERNAL__$name merge 20 acct-stats"
  echo "service-policy output INTERNAL__$name merge 20 acct-stats"
  echo "accounting aaa list ibs-isg type service periodic-interval $interm"
 echo 'ipv4 access-group INTERNAL ingress'
  echo 'ipv4 access-group INTERNAL egress'

done < $file

echo 'exit'

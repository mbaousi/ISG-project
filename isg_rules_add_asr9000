#!/bin/bash
psql -U ibs IBSng -c "copy (select DISTINCT attr_value from charge_rule_attrs where attr_name = 'limit_bw')  To '/tmp/service.csv' With CSV DELIMITER ',' ; "
cat /tmp/service.csv | cut -d , -f2 | cut -d '"' -f1 > /tmp/a
sleep 2

file="/tmp/a"

echo 'Please Enter IBSng ip address:'
read radius

echo 'Please Set update periodic acc (MIN):'
read interm

echo 'Please unnumber loopback : '
read unnum


echo 'aaa group server radius ibs-isg'
echo "server $radius auth-port 1812 acct-port 1813"


echo 'aaa accounting service IBSng group IBSng'
echo 'aaa accounting service ibs-isg group ibs-isg'
echo 'aaa accounting subscriber IBSng group IBSng'
echo 'aaa accounting subscriber ibs-isg group ibs-isg'
echo 'aaa authorization subscriber IBSng group IBSng'
echo 'aaa authorization subscriber default group radius'
echo 'aaa authorization subscriber ibs-isg group ibs-isg'
echo 'aaa authentication subscriber IBSng group IBSng'
echo 'aaa authentication subscriber default group IBSng'
echo 'aaa authentication subscriber ibs-isg group ibs-isg'
echo 'aaa authorization prepaid default group ibs-isg'
echo 'aaa authorization prepaid ibs-isg group ibs-isg'
echo 'subscriber'
echo 'manager'
echo 'trace level verbose location 0/RSP0/CPU0'
echo '!'
echo '!'
echo 'exit'
echo 'exit'
echo 'aaa authorization network default group ibs-isg'
echo 'aaa authorization network ibs-isg group ibs-isg'
echo 'aaa authentication ppp default group ibs-isg'
echo 'aaa authentication ppp ibs-isg group ibs-isg'
echo "aaa accounting update periodic $interm"



echo 'dynamic-template'
 echo 'type ppp BNG'
  echo 'ppp authentication pap chap ms-chap'
  echo 'keepalive 60 5'
  echo 'ppp mru ignore'
  echo 'ppp ipcp dns 217.218.127.127 4.2.2.4'
  echo "accounting aaa list ibs-isg type session periodic-interval $interm"
  echo "ipv4 unnumbered $unnum"
  echo '!'
  echo 'type ppp PPP'
  echo 'ppp authentication pap chap ms-chap'
  echo 'keepalive 60 5'
  echo 'ppp mru ignore'
  echo 'ppp ipcp dns 217.218.127.127 8.8.8.8'
  echo "accounting aaa list ibs-isg type session periodic-interval $interm"
  echo "ipv4 unnumbered $unnum"
  echo 'ipv4 unreachables disable'
  echo '!'
 echo 'type service IXP'
  echo "accounting aaa list IBSng type service periodic-interval $interm"
  echo 'ipv4 access-group IXP ingress'
  echo 'ipv4 access-group IXP egress'
 echo '!'
 echo 'type service TCI'
  echo "accounting aaa list IBSng type service periodic-interval $interm"
  echo 'ipv4 access-group TCI ingress'
  echo 'ipv4 access-group TCI egress'
